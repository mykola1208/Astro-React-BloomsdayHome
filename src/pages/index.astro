---
import Layout from "../layouts/Layout.astro";
import { Icon } from "astro-icon/components";
---

<Layout title="Bloomsday">
  <div class="py-9 px-6">
    <p class="neue text-darkgreen text-lg font-bold leading-5">
      Home > ProcessName
    </p>
    <div class="pr-4 pt-2 pb-4 gap-4 mt-9 flex">
      <Icon name="myhouse" width={58} height={55} />
      <div class="flex flex-col">
        <div class="flex">
          <p class="text-base leading-4 text-darkgreen mt-3.5">My Home</p>
          <div class="flex mt-2 ml-2">
            <Icon name="edit" width={20} height={21} />
          </div>
        </div>

        <p class="leading-6 text-2xl font-medium text-darkgreen mt-1.5">
          Street, City, State 00000
        </p>
      </div>
    </div>
  </div>
</Layout>
